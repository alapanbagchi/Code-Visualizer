{
  "dependencies": {
    "amqplib": "^0.10.8",
    "axios": "^1.10.0",
    "buffer": "^6.0.3",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "pg": "^8.16.2",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cpx \"src/**/*.{py,json}\" dist",
